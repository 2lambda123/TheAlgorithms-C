#include <stdio.h>

long int subArraySum(int arr[], int n)
{
    long int result = 0;
    int i =0,j=0, k= 0;
    for (i=0; i <n; i++)
    {
        for (j=i; j<n; j++)
        {
            for (k = i ; k <= j ; k++)
            {
                result += arr[k];
            }
        }
    }
    return result ;
}
int main()
{
    int arr[] = { 1,3,5};
    int n = ARRAY_SIZE(arr);
    long int sum  = subArraySum(arr, n) ;
    printf("Sub Array Sum = %d\n",sum);
    return 0;
}
